{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/better-docs/typescript"

  ],
  "typescript": {
    "moduleRoot": "test"
  },
  "templates": {
    "better-docs": {
      "name": "Recipe Server"
    }
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "../client/api-docs",
    "recurse": true,
    "readme": "README.md"
  }
}
